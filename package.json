{
  "name": "tdd-explore",
  "version": "1.0.0",
  "description": "exploring tdd concepts",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "build": "webpack --colors --progress",
    "dev": "webpack-dev-server",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --watch --require babel-register",
    "test:coverage": "nyc --cache npm test"
  },
  "nyc": {
    "check-coverage": true,
    "functions": 80,
    "lines": 80,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "build"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelassumpcao/tdd_explore.git"
  },
  "keywords": [
    "tdd",
    "js",
    "study"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafaelassumpcao/tdd_explore/issues"
  },
  "homepage": "https://github.com/rafaelassumpcao/tdd_explore#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
